{
  "name": "gv-app",
  "private": true,
  "type": "module",
  "main": "index.cjs",
  "scripts": {
    "build": "tsc --outDir dist && vite build",
    "dev": "concurrently \"tsc --outDir dist\" \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "prestart": "tsc --outDir dist",
    "typecheck": "react-router typegen && tsc",
    "react-dev": "vite"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0",
    "react-router-dom": "^7.3.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "concurrently": "^8.2.2",
    "electron": "^25.9.8",
    "electron-builder": "^24.13.3",
    "react-router-devtools": "^1.1.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.example.app",
    "files": [
      "dist/**/*",
      "main.ts",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
